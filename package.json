{
  "name": "homeless-risk-dashboard",
  "version": "0.0.1",
  "description": "GraphQL Homeless Risk Dashboard",
  "main": "src/server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "prod-build-client": "webpack --config ./webpack.config.js",
    "prod-build-server": "babel ./src -d $OUTPUT_DIR/server --source-maps",
    "prod-build": "npm run clean && npm run prod-build-client && npm run prod-build-server",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build ; fi",
    "start": "node $OUTPUT_DIR/server/server",
    "predev-start": "npm run clean",
    "webpack-dev-server": "webpack-dev-server --config webpack.config.js",
    "dev-server": "nodemon -e js,jsx,md,mustache -w ./server --exec ./dev-tools/babel-run -- ./server",
    "dev-start": "concurrently --kill-others 'npm run dev-server' 'npm run webpack-dev-server'",
    "dev": "nf start -w --procfile ./dev-tools/Procfile.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anichale/homeless-risk-dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Anichale/homeless-risk-dashboard/issues"
  },
  "homepage": "https://github.com/Anichale/homeless-risk-dashboard#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "concurrently": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "foreman": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "nodemon": "^1.10.2",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "apollo-client": "^0.4.14",
    "apollo-server": "^0.2.8",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "casual": "^1.5.5",
    "co": "^4.6.0",
    "cors": "^2.8.1",
    "dataloader": "^1.2.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0",
    "graphql-tag": "^0.1.13",
    "graphql-tools": "^0.6.6",
    "grommet": "^0.6.11",
    "grommet-index": "^0.4.5",
    "grommet-templates": "^0.2.0",
    "grommet-toolbox": "^0.4.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "mongodb": "^2.2.10",
    "react": "^15.3.1",
    "react-apollo": "^0.5.4",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.0",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-manifest-plugin": "^1.0.1"
  }
}
